        <activity android:name=".activity.myactivity.MySettingActivity" />
        <activity android:name="com.wd.tech.activity.myactivity.MyUpdateUserMessageActivity" />
        <activity android:name=".activity.myactivity.SignActivity" />
        <activity android:name=".activity.InterestActivity" />
        <activity android:name=".activity.PictureDisplayActivity" />
        <activity android:name=".activity.WebDetailsActivity" />
        <activity android:name=".activity.InterestDetailsActivity" />
        <activity android:name=".activity.AddFriendActivity" />
        <activity android:name=".activity.AddFriendsActivity" />
        <activity android:name=".activity.SearchActivity" />
        <activity
            android:name=".activity.InformationDetailsActivity"
            android:windowSoftInputMode="adjustPan" />
        <activity
            android:name=".activity.FridendAddActivity"
            android:windowSoftInputMode="adjustPan" />
        <meta-data
            android:name="UMENG_APPKEY"
            android:value="5c7d1cbe61f56448cc00012c" />
        <meta-data
            android:name="UMENG_CHANNEL"
            android:value="${UMENG_CHANNEL_VALUE}" />
        <activity android:name=".activity.CreateGroupActivity" />
        <activity android:name=".activity.OpenVipActivity" />
